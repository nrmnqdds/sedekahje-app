{
  "expo": {
    "name": "SedekahJe",
    "slug": "sedekahje",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/ios/AppIcon@3x.png",
    "scheme": "sedekahje",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nrmnqdds.sedekahje",
      "icon": "./assets/app-icons/ios/AppIcon@3x.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "./assets/app-icons/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "monochromeImage": "./assets/app-icons/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png"
      },
      "package": "com.nrmnqdds.sedekahje",
      "icon": "./assets/app-icons/android/play_store_512.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/app-icons/web/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sedekahje-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.4"
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SedekahJe to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "881ff1f5-b5f7-44a0-addd-489b8be72c54"
      }
    }
  }
}
